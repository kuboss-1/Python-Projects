#factorial

fact = 1
number = int(input("Please enter a number: "))
for i in range(1, number + 1):
  fact = fact * i
print("The factorial of", number, "is", fact)

#This program checks if the last and the first terms are the same

numbers = input("Please write your number set: ")
def same_nums(numbers):
    number_list = numbers.replace(" ", "").split(",")
    return number_list[0] == number_list[-1]
print(same_nums(numbers))

#This finds the numbers that can be divided by 5

numbers = input("Please write your number set: ")

def divide(numbers):
    number_list = numbers.replace(" ", "").split(",")
    result = []
    
    
    for num in number_list:
        num = int(num)
        if num % 5 == 0:
            result.append(str(num))
    return result

#This function combines two lists and their 
list1 = ["M", "na", "i", "Ke"] 
list2 = ["y", "me", "s", "lly"]

for i in range(len(list1)):
    list1[i] += list2[i]

print(list1)


